# Turn mod_rewrite on

RewriteEngine On

#Modify Urls for Page Navigation

RewriteRule ^Login?$ index.php [NC,L]
RewriteRule ^Dashboard?$ dashboard.php [NC,L]
RewriteRule ^Add-post?$ add_post.php [NC,L]
RewriteRule ^View-posts?$ view_posts.php [NC,L]

RewriteRule ^Manage-Categories?$ view_cat.php [NC,L]
RewriteRule ^Manage-users?$ man_users.php [NC,L]

RewriteRule ^Logout?$ logout.php [NC,L]